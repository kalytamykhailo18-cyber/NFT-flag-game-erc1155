{
  "name": "municipal-place-nft-1155-backend",
  "version": "1.0.0",
  "description": "ERC-1155 Place NFT Game Backend",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "update:images": "node scripts/updatePlaceImages.js",
    "update:images:all": "node scripts/updatePlaceImages.js --all",
    "update:images:dry": "node scripts/updatePlaceImages.js --dry-run",
    "update:images:test": "node scripts/updatePlaceImages.js --limit 2",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "form-data": "^4.0.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "@pinata/sdk": "^2.1.0",
    "sequelize": "^6.35.2",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
